page{background-color:#f2f2f2}.section{background-color:#fff;padding:30rpx;font-size:28rpx;margin-bottom:20rpx}.section .section_title{font-size:28rpx;color:#333333;padding-bottom:30rpx}.section .row{display:flex;justify-content:space-between;margin-bottom:20rpx}.section .row input{border-bottom:2px solid rgba(0,0,0,0.12)}.section .row .placeholder{color:#989898}.section .row .long_input{width:100%}.section .row .btn{width:335rpx;height:60rpx;border-radius:30rpx;font-size:28rpx;color:#ffffff;text-align:center;line-height:60rpx;margin-top:30rpx}.section .row .weight{background-color:#7897f4}.section .row .high{background-color:#ffab03}.section .content{border:1px solid #ccc;padding:10rpx}.section .content textarea{font-size:26rpx;line-height:36rpx;color:#333333;width:100%}.section .content .photo_area{padding-top:20rpx;display:flex;align-items:center;justify-content:flex-start;flex-wrap:wrap}.section .content .photo_area .photo{display:flex;flex-direction:column;align-items:center;margin:0 20rpx 10rpx 20rpx;position:relative}.section .content .photo_area .photo image{max-width:120rpx;max-height:120rpx}.section .content .photo_area .photo .date{font-size:20rpx;color:#656565}.section .content .photo_area .photo icon{position:absolute;right:-20rpx;top:-18rpx;z-index:12}.section .history_case{border:none}
